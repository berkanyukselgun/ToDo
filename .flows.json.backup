    {
        "id": "250991c5b6a78d62",
        "type": "function",
        "z": "205ea1bbd9a0c513",
        "name": "db.user.opt()",
        "func": "const DB = global.get(\"DB\")\n\nlet user = DB.user\nconst date = new Date()\nconst now = date.getTime()\n\nif (msg.req.params.type === \"add\") {\n\n    user.push({\n        id: now,\n        active: true,\n        stamp: {\n            createdAt: now,\n            ip: msg.req.ip,\n            user: \"\",\n            email: \"\",\n        },\n        date: date.toLocaleDateString(\"TR\"), //DD.AA.YYYY\n        time: date.toLocaleTimeString(\"TR\"), //SS:DD:NN\n        ...msg.payload\n    })\n}\nelse {\n    const index = user.findIndex(x => x.id == msg.payload.id)\n\n    if (!~index) {\n\n        msg.error = {\n            message: \"id ile eşleşen bir kayıt bulunamadı.\"\n        }\n        return [null, msg]\n    }\n\n    user[index] = { ...user[index], ...msg.payload };\n\n    user[index].stamp = {\n        createdAt: now,\n        ip: msg.req.ip,\n        user: \"\",\n        email: \"\",\n    }\n\n    user[index].date = date.toLocaleDateString(\"TR\") //DD.AA.YYYY\n    user[index].time = date.toLocaleTimeString(\"TR\") //SS:DD:NN\n\n}\n\nDB.user = user\nglobal.set(\"DB\", DB)\n\nreturn msg\n\n\n",
